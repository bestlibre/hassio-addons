{
    "name": "Aria2",
    "version": "0.4",
    "slug": "aria2",
    "description": "Aria2 download manager",
    "url": "https://github.com/bestlibre/hassio-addons/tree/master/aria2",
    "startup": "system",
    "arch": ["armv7", "armhf", "amd64", "aarch64", "i386"],
    "boot": "auto",
    "image": "bestlibre/{arch}-aria2",
    "options": {"options":[]},
    "schema": {"downloads": "str?", "secret": "str?", "options":[{"name": "str", "value": "str"}]},
    "map": ["share:rw"],
    "webui": "http://[HOST]:[PORT:8080]",
    "ingress": true,
    "ingress_port": 8080,
    "panel_icon": "mdi:download",
    "ports": {"6800/tcp": 6800, "8080/tcp": null},
    "ports_description": {
      "6800/tcp": "Aria2 RPC port",
      "8080/tcp": "Aria2 WebUI interface"
  }
}
